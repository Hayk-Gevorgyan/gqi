{
	"name": "@relcu/gqi",
	"private": false,
	"version": "1.2.1",
	"description": "Devtools GraphQL network inspection plugin",
	"type": "module",
	"files": [
		"./dist"
	],
	"scripts": {
		"dev": "vite",
		"watch": "vite build -w",
		"build": "vue-tsc --noEmit && vite build",
